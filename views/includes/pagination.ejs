<section class = "pagination">
    <% if (currentPage != 1 && previousPage != 1){%>
        <a href="?page=1">1 - First Page</a>
    <%}%>
    <% if (hasPreviousPage){%>
        <a href="?page=<%=previousPage%>"><%=previousPage%> Previous Page</a>
    <%}%>
    <a href="?page=<%=currentPage%>" class = "active"><%= currentPage %> - Current Page</a>
    <% if (hasNextPage){%>
        <a href = "?page=<%= nextPage%>"><%=nextPage%>- Next Page</a>
    <%}%>
    <% if (lastPage != currentPage && nextPage != lastPage){%>
        <a href="?page=<%=lastPage%>"><%=lastPage%> - Last Page</a>
    <%}%>
</section>